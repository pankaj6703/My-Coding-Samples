<script>
function showLargeImage()
{

$("#divLargeImage").css('display','block');
$("#but_link").hide();	$.ajax({
               	url : '{{ path("raa_web_airlineDetail") }}',
                type:'POST',
                data:'stateCode='+id,
                dataType:'html',   
                success:function(data){
              
                    //var category_product = $(data).find('.city').html();
                    $('#divAirlineDetail').css('display','block');
                    $('#divAirlineDetail').html(data);

                }
            });


}
$("#cross").hide();
}

</script>
<script>
function searchArilineByName(a)
{
if(a!="")
{
$("#divAjaxResults").show();
$.ajax({
               	url : '{{ path("raa_web_getAirlineName",{ "id": '+a+' }) }}',
                type:'POST',
                data:'id='+a,
                dataType:'html',   
                success:function(data){
                
                    //var category_product = $(data).find('.city').html();
                   $('.ajx_div').html(data);
//alert(data);
//document.getElementById("cross").innerHTML=data;
                }
            });

}
else
{
$('.ajx_div').html('');
$("#divAjaxResults").hide();
}


}
function updateCities(id)
{
 
            //   alert(id);              	
               	
               	$.ajax({
               	url : '{{ path("raa_web_airlineDetail") }}',
                type:'POST',
                data:'stateCode='+id,
                dataType:'html',   
                success:function(data){
                 // alert(data);
                    //var category_product = $(data).find('.city').html();
               		//alert(data);
                    $('#divAirlineDetail').css('display','block');
                    $('#divAirlineDetail').html(data);

                }
            });


}

function updateSearchValue(value)
{
$("#search").val(value);
updateCities(value);

$("#divAjaxResults").hide();
}



</script>
<script>
function showLargeImage()
{

$("#divLargeImage").css('display','block');
$("#but_link").hide();
$("#cross").hide();
}

</script>

 

