	<script>
$(document).ready(function(){

updateCities('AK');
		
		$("#state").change(function(){	
               var id=$(this).val();
             updateCities(id);  

        });	
});

function updateCities(id)
{
 
                //alert(id);              	
               	
               	$.ajax({
               	url : '{{ path("RAAAdminBundle_getCities",{ "stateCode": '+id+' }) }}',
                type:'POST',
                data:'stateCode='+id,
                dataType:'html',   
                success:function(data){
                    //var category_product = $(data).find('.city').html();
                    $('#city').html(data);

                }
            });


}
		
</script>	
			 <script>
         		$(document).ready(function()
         		{
         				$(".active").removeClass("active");
         				var url = window.location.href;
         				if( url.indexOf('/dashboard') > -1 )
 									$("#Dashboard").addClass("active");
 								if( url.indexOf('/airlines') > -1 )
 									$("#Realtors").addClass("active");
 								if( url.indexOf('/category') > -1 )
 									$("#Categories").addClass("active");
 								if( url.indexOf('/plan') > -1 )
 									$("#Plans").addClass("active");
 											if( url.indexOf('/claim') > -1 )
 									$("#request").addClass("active");
 											if( url.indexOf('/propertyListing') > -1 )
 									$("#property").addClass("active");
 															if( url.indexOf('/cms') > -1 )
 									$("#cms").addClass("active");
 									if( url.indexOf('/advertiesment') > -1 )
 									$("#adv").addClass("active");
 										if( url.indexOf('/reviewer') > -1 )
 									$("#rev").addClass("active");
 									if( url.indexOf('/manageReviews') > -1 )
 									$("#manRev").addClass("active");
