          <script>
            function swapImages() 
            {
           //alert("hi");
            
							var $current = $('#myGallery img:visible');
							var $next = $current.next();
    					if($next.length === 0) 
    					{
        				$next = $('#myGallery img:first');
    					}
    					$current.hide();
    					$next.show();
						}
						
		
						
	$(document).ready(function() 
					{
    			setInterval(swapImages, 5000);
				});
				
	
	
				
				
</script>
          
