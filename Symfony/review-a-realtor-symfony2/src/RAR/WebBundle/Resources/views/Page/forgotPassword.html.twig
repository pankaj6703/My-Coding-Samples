{% extends 'RARWebBundle::layout.html.twig' %}
{% block body %}
<title>{% block title %}Forgot Password {% endblock %}</title>
	<div itemscope itemtype="http://schema.org/Product" itemref="_url2 _brand3" class="main"> <!-- search by state -->
		<div class="cl"></div>
		<!-- write review -->
		<div class="titles oranges">
			<h2>Realtor Forgot Password</h2>
		</div>
		<!--login Box-->
		<div id="login">
			<form action="{{ path('rar_web_forgotpassword') }}" id="loginform" method="post">
			<label for="name">Email:</label>
			<input type="name" id="email" name="email" autofocus>
			{% if name1 is defined%}
			<div class="error1">
				{{name1}}
				{%endif%}
				<div id="lower">
					<input type="submit" value="Submit">
				</div>	
			</form>
			<div class="cl">
			</div>
		</div>
	</div>








{% endblock %}
