{% block body %}
<title>{% block title %} Manage Realtors | Admin{% endblock %}</title>
	<section id="content">
	<div class="g12">
		<h1>Manage Realtors</h1>
		<button class="btnAddNew" onclick="javascript:addNewPlan();">Add New</button>
		<button class="btnAddNew" onclick="javascript:importUsers();">Import Realtors</button>
		<button class="btnAddNew" onclick="javascript:importUsers1();">Export Realtors in Csv</button>
		<!--<button class="btnAddNew" onclick="javascript:getPdf();">Export Realtors in Pdf</button>-->
		<div class="dataTables_wrapper">
			<table class="datatable">
			<thead>
				<tr>
					<th class="sorting col-2" style="width: 58px;" rowspan="1" colspan="1"> # &nbsp;&nbsp;</th>
					<th class="sorting col-3" style="width: 109px;" rowspan="1" colspan="1">Name &nbsp;&nbsp;</th>
					<!--<th class="sorting col-3" style="width: 104px;" rowspan="1" colspan="1">LastName &nbsp;&nbsp;</th>-->
					<th class="sorting col-2" style="width: 120px;" rowspan="1" colspan="1">Email &nbsp;&nbsp;</th>
					<th class="sorting col-2" style="width: 75px;" rowspan="1" colspan="1">Phone &nbsp;&nbsp;</th>
					<th class="sorting col-6" style="width: 240px;" rowspan="1" colspan="1">Address &nbsp;&nbsp;</th>
					<th class="sorting col-2" style="width: 77px;" rowspan="1" colspan="1">City &nbsp;&nbsp;</th>
					<th class="sorting col-3" style="width: 75px;" rowspan="1" colspan="1">State &nbsp;&nbsp;</th>
					<!--<th class="sorting col-2" rowspan="1" colspan="1" style="width: 180px;">Country</th>-->
					<th class="sorting col-3" style="width: 48px;" rowspan="1" colspan="1">Zip &nbsp;&nbsp;</th>
					<th class="col-2" id="colAction"style="width:100px;" rowspan="1" colspan="1"><label class="lblAction">Action &nbsp;&nbsp;</label></th>
				</tr>
			</thead>
			<tbody>		
				{% for realtor in realtors %}
				<tr class="gradeA odd">
					<td class="">{{realtor.id}}</td>
					<td class="txtleft">{{realtor.first_name}} {{realtor.last_name}}</td>

					<td class="txtleft">{{realtor.email}}</td>
					<td class="txtleft">{{realtor.phone}}</td>
					<td class="txtleft">{{realtor.address}},{{realtor.address2}}</td>
					<td class="txtleft">{{realtor.city_name}}</td>
					<td class="txtleft">{{realtor.state_name}}</td>
					<!--<td class="txtleft">{{realtor.country}}</td>-->
					<td class="txtleft">{{realtor.pincode}}</td>
					<td><a href="{{ path('RARAdminBundle_realtorupdate',{ 'id': realtor.id }) }}"><img src="{{ asset('bundles/images/pencil.png') }}"></image></a><a href="{{ path('RARAdminBundle_realtordelete', { 'id': realtor.id }) }}"><img src="{{ asset('bundles/images/cross.png') }}" onclick="confirm_Delete(event);"></image></a>
					
					<!--<a href= "{{ path('RARAdminBundle_suspenedRealtors', { 'id': realtor.id }) }}"><img src="{{ asset('bundles/images/suspened.png') }}"></image></a>-->
					
					
					</td>			
				</tr>
				{% endfor %}
			</table>

			
			</form>
		</div>
	</div>	
{% endblock %}
